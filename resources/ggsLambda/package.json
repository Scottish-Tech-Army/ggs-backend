{
  "name": "ggs-lambda",
  "version": "2.0.0",
  "description": "CDK GGS Lambda function",
  "main": "dist/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "test": "jest",
    "tsc": "tsc",
    "build": "esbuild src/index.ts --bundle --platform=node --target=node14 --outfile=dist/index.js --external:canvas --external:sharp --external:chartjs-node-canvas --external:./node_modules/chartjs"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.48.0",
    "@aws-sdk/util-dynamodb": "^3.48.0",
    "@babel/runtime": "^7.18.3",
    "aws-lambda": "^1.0.7",
    "es6-promise": "^4.2.8",
    "isomorphic-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.5",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@types/aws-lambda": "^8.10.100",
    "@types/jest": "^28.1.2",
    "babel-jest": "^28.1.1",
    "esbuild": "^0.14.45",
    "eslint": "^8.18.0",
    "eslint-plugin-jest": "^26.5.3",
    "jest": "^28.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "plugins": [
            "@babel/plugin-transform-runtime"
          ],
          "presets": [
            "@babel/preset-env",
            [
              "@babel/preset-typescript",
              {
                "parserOpts": {
                  "strictMode": true
                }
              }
            ]
          ]
        }
      ]
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "json",
      "node"
    ],
    "maxWorkers": "50%",
    "setupFilesAfterEnv": ["./setupTests.ts"]
  }
}
